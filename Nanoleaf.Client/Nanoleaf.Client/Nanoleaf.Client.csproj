<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <Version>1.1.2</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>A cross-platform library that takes advantage of Nanoleaf API.</Description>
    <PackageProjectUrl>https://github.com/BullFrog13/Nanoleaf-Client</PackageProjectUrl>
    <RepositoryUrl>https://github.com/BullFrog13/Nanoleaf-Client</RepositoryUrl>
    <PackageReleaseNotes>A cross-platform library that takes advantage of Nanoleaf API.</PackageReleaseNotes>
    <PackageTags>nanoleaf cross platform tkachenko http api web core yevgeniy</PackageTags>
    <PackageId>Nanoleaf.Core</PackageId>
    <Authors>Tkachenko Yevgeniy</Authors>
    <Product>Nanoleaf.Core</Product>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>
