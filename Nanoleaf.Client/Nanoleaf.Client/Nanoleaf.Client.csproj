<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <Version>1.1.6.2</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>A cross-platform library for Nanoleaf API.</Description>
    <PackageProjectUrl>https://github.com/BullFrog13/Nanoleaf-Client</PackageProjectUrl>
    <RepositoryUrl>https://github.com/BullFrog13/Nanoleaf-Client</RepositoryUrl>
    <PackageReleaseNotes>A cross-platform library for Nanoleaf API.</PackageReleaseNotes>
    <PackageTags>nanoleaf api core</PackageTags>
    <PackageId>Nanoleaf.Core</PackageId>
    <Authors>Tkachenko Yevgeniy</Authors>
    <Product>Nanoleaf.Core</Product>
    <AssemblyVersion>1.1.6.2</AssemblyVersion>
    <FileVersion>1.1.6.2</FileVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryType>git</RepositoryType>
	<GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DeviceDiscovery" Version="1.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="9.0.1" />
  </ItemGroup>

</Project>
